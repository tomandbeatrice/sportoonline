{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "analyze": "vite build && npx -y source-map-explorer ./dist/assets/*.js",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ext .ts,.vue,.js src/",
    "lint:fix": "eslint --ext .ts,.vue,.js src/ --fix",
    "prettier": "prettier --write \"src/**/*.{ts,vue,js,json,css,scss}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,vue,js,json,css,scss}\"",
    "type-check": "vue-tsc --noEmit",
    "sonar": "sonar-scanner",
    "sonar:local": "sonar-scanner -Dsonar.host.url=http://localhost:9000",
    "lint:sonar": "npm run lint && npm run sonar",
    "tailwind:init": "tailwindcss init",
    "tailwind:watch": "tailwindcss -i ./src/assets/tailwind.css -o ./src/assets/output.css --watch"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.5.0",
    "@types/vue-router": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "baseline-browser-mapping": "^2.8.29",
    "concurrently": "^9.0.1",
    "daisyui": "^5.0.50",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "jsdom": "^26.1.0",
    "laravel-vite-plugin": "^2.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.13",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.8"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@types/lodash": "^4.17.20",
    "@vueuse/core": "^14.1.0",
    "@vueuse/head": "^2.0.0",
    "chart.js": "^4.5.1",
    "driver.js": "^1.3.6",
    "html2pdf.js": "^0.10.3",
    "laravel-echo": "^2.2.6",
    "lodash": "^4.17.21",
    "lucide-vue-next": "^0.560.0",
    "marked": "^17.0.0",
    "pinia": "^3.0.3",
    "pusher-js": "^8.4.0",
    "shadcn-ui": "^0.9.5",
    "socket.io-client": "^4.8.1",
    "storybook": "^10.0.6",
    "vue": "^3.5.21",
    "vue-chartjs": "^5.3.3",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.5.1",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-toastify": "^0.2.8",
    "vuedraggable": "^4.1.0"
  }
}
