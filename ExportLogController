public function index()
{
    $logs = ExportLog::latest()->take(20)->get();

    return response()->json([
        'status' => 'success',
        'data' => $logs
    ]);
}
public function startExport()
{
    // Export işlemini başlat
    $result = ExportService::run();

    // Logları topla
    $logs = ExportLog::latest()->take(20)->get();

    return response()->json([
        'status' => $result ? 'success' : 'error',
        'logs' => $logs
    ]);
}