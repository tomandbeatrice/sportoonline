public function storePlannedCampaign(Request $request)
{
    PlannedCampaign::create([
        'code' => $request->code,
        'title' => $request->title,
        'discount_rate' => $request->discount,
        'commission_drop' => $request->commission,
        'start_date' => $request->start,
        'end_date' => $request->end
    ]);

    return response()->json(['message' => 'Kampanya planlandÄ±.']);
}